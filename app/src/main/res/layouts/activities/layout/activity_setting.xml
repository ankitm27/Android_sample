                        <ImageView
                            
                            android:id="@+id/th"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="@dimen/_6dp"
                            android:layout_weight="22"
                            android:gravity="center"
                            android:text="TH" />

                        <ImageView
                            android:id="@+id/lakh"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="@dimen/_6dp"
                            android:layout_weight="22"
                            android:gravity="center" />

                        <ImageView
                            android:id="@+id/cr"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="@dimen/_6dp"
                            android:layout_weight="22"
                            android:gravity="center" />
                    </LinearLayout>

                </LinearLayout>

